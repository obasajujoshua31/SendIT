{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "spread": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-restricted-syntax": "off",
    "linebreak-style": "off",
    "no-console": "off",
    "object-curly-newline": "off",
    "arrow-parens": "off",
    "operator-linebreak": "off",
    "comma-dangle": "off"
  }
}
