{
  "name": "send-it",
  "version": "1.0.0",
  "description": "A Parcel delivery application",
  "main": "app.js",
  "scripts": {
    "test": "rimraf .nyc_output/ && rimraf .coverage/ && nyc mocha --require babel-register ",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./coverage,./node_modules,./Procfile,./README.md,./.babelrc,./test,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/app.js",
    "coverage": "nyc report --reporter=html"
  },
  "engines": {
    "node": "10.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obasajujoshua31/SendIT"
  },
  "author": "Obasaju Joshua",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4",
    "express-validator": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0"
  }
}
